<?xml version="1.0"?>
<package >
  <metadata minClientVersion="3.2">
    <id>XBee</id>
    <version>5.2.0</version>
    <authors>Jeff Haynes</authors>
    <owners></owners>
    <projectUrl>https://github.com/jefffhaynes/XBee</projectUrl>
