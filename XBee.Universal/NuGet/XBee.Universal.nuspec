    <file src="..\bin\Reference\Release\XBee.Universal.dll" target="ref\uap10.0" />

    <!-- XML documentation file goes together with the reference library. -->
    <file src="..\bin\x86\Release\XBee.Universal.xml" target="ref\uap10.0" />

    <!-- Resource files go together with the reference library. -->
    <file src="..\bin\x86\Release\XBee.Universal.pri" target="ref\uap10.0" />

    <!-- The architecture-specific files go in architecture-specific directories. -->
    <file src="..\bin\x86\Release\XBee.Universal.dll" target="runtimes\win10-x86\lib\uap10.0" />
    <file src="..\bin\x86\Release\XBee.Universal.pdb" target="runtimes\win10-x86\lib\uap10.0" />

    <file src="..\bin\x64\Release\XBee.Universal.dll" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\bin\x64\Release\XBee.Universal.pdb" target="runtimes\win10-x64\lib\uap10.0" />

    <file src="..\bin\arm\Release\XBee.Universal.dll" target="runtimes\win10-arm\lib\uap10.0" />
