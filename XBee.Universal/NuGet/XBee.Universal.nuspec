    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A UWP and .NET classic library for controlling XBee wireless modules.</description>
    <releaseNotes>Core.XBeeController is now named Core.XBeeControllerBase and is abstract.</releaseNotes>
    <tags>XBee Wireless</tags>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <dependencies>
      <dependency id="XBee.Core" version="1.3.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\XBee.Classic\bin\Release\XBee.Classic.dll" target="lib\net46" />
    
